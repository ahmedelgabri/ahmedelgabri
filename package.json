{
	"name": "ahmedelgabri",
	"author": "Ahmed El Gabri <ahmed@gabri.me> (https://gabri.me)",
	"version": "7.0.0",
	"description": "A personal card for Ahmed El Gabri (@ahmedelgabri)",
	"license": "MIT",
	"homepage": "https://gabri.me",
	"main": "index.mjs",
	"type": "module",
	"bin": {
		"ahmedelgabri": "./bin/card.cjs"
	},
	"scripts": {
