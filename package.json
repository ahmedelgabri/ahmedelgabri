	},
	"devDependencies": {
		"@ahmedelgabri/prettier-config": "^7.0.0",
		"esbuild": "^0.17.16",
		"prettier": "^2.8.7",
		"standard": "^17.0.0"
	},
	"dependencies": {
		"boxen": "^7.0.2",
		"picocolors": "^1.0.0"
